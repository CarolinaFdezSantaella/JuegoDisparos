    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.shapeshooter.game"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
