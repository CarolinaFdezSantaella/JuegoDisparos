{
  "expo": {
    "name": "Shape Shooter Showdown",
    "slug": "shape-shooter-showdown",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
